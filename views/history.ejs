<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/normalize.css" />
    <link rel="stylesheet" href="/skeleton.css" />
    <link rel="stylesheet" href="/style.css">
    <title>Document</title>
</head>

<body>
    <h1>History</h1>
    
    <div class="data">
    <% meditations.forEach(meditation => { %>
      <a id="history-date" href="/session/<%=meditation._id %>">  Date: <%= meditation.date %> <br></a>
      
        <form id="history" action="/session/<%=meditation._id %>?_method=DELETE" method="POST">
            <input class="delete-btn" type="submit" value="DELETE" /> 
        </form>
        <button id="history-back" onclick="window.location.href='/session'">Back</button>
        
        <% }) %>
    </div>
</body>

</html>